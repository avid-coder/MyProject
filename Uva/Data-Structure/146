#include <bits/stdc++.h>
using namespace std;

int main() {
    string ana;
    while(cin >> ana) {
        if(ana != "#") {
            bool flag = false;
            if (next_permutation(ana.begin(), ana.end())) {
                flag = true;
                cout << ana << endl;
            }
            if(!flag) {
                cout << "No Successor" << endl;
            }
        }
    }
    return 0;
}
