#include <bits/stdc++.h>
using namespace std;

int main() {
    int num;
    vector <int> v;
    while(cin >> num) {
        v.push_back(num);
        sort(v.begin(), v.end());
        if(v.size() % 2 != 0) {
            cout << v[v.size()  / 2] << endl;
        }
        else {
            cout << (v[(v.size() / 2)] + v[(v.size() - 1 )/ 2]) / 2 << endl;
            
            }
        
    }

    return 0;
}
